enum ErrCode {
    ErrOk = 0
    PARAM_ERROR = 1003
}

type Response<T> {
    ErrCode errno
    string errmsg
    T data
}

type ManagerReq {
    string id
}

type Manager {
    string name
    int? age
}

rpc GetManager(ManagerReq) ResponseManager {
    method="GET"
    path="/user"
}

type ResponseManager Response<Manager?>