// stream.idl

type StreamReq {
    string id
}

type StreamResp {
    string id
    string data
    Payload payload
}

oneof Payload {
    string text_data
    int? numberData (json="number_data")
    bool boolean_data (json="")
}

rpc Stream(StreamReq) stream<StreamResp> {
    method="GET"
    path="/stream"
}